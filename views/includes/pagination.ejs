<nav aria-label="Page navigation">
    <ul class="pagination justify-content-center">
        <% if(currentPage !==1 && previousPage !==1 ) {%>
            <li class=""><a href="?page=1">1</a></li>
            <%}%>
                <%if(hasPeriviousPage){%>
                    <li class=""><a href="?page=<%= previousPage %>">
                            <%= previousPage %>
                        </a></li>
                    <%}%>
                        <li><a href="?page=<%=currentPage%>">
                                <%=currentPage%>
                            </a></li>

                        <%if(hasNextPage){%>
                            <li><a href="?page=<%= nextPage %>">
                                    <%= nextPage %>
                                </a></li>
                            <%}%>
                                <% if(lastPage !==currentPage && nextPage!==lastPage) {%>
                                    <li><a href="?page=   <%= lastPage %>">
                                            <%= lastPage %>
                                        </a></li>
                                    <%}%>
    </ul>
</nav>